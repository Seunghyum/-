{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": ["./src"],
    "includePattern": ".js$", 
    "excludePattern": "(node_modules/)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "template": "node_modules/docdash",
    "encoding": "utf8",
    "destination": "./jsdoc",
    "recurse": true,
    "verbose": true,
    "readme": "README.md"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": false
    }
  },
  "docdash": {
    "static": false,
    "sort": false
  }
}
